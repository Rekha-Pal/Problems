#include <stdio.h>
int main()
{
    int n,count=0;
    scanf("%d",&n);
    while(n)
    {
        count=0;
        while(n>1)
        {
            if(n%2)
                n=n-(n+1)/2;
            else
                n/=2;
            count++;
        }
        printf("%d\n",count);
        scanf("%d",&n);

    }
    return 0;
}
